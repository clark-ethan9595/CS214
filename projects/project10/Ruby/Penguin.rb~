####################################################

require './Bird.rb'

class Penguin < Bird

	######################################################
	# call returns the sound that a Penguin type makes   #
	# Recieve: NONE                                      #
	# Return: Huh-huh-huh-huuuuuuh!, a String            #
	######################################################
	def call
		"Huh-huh-huh-huuuuuuuuh!"
	end

end
